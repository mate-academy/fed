{
  "name": "@mate-academy/lint",
  "version": "0.1.1",
  "main": "bin/matelint.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mate-academy/fed.git"
  },
  "keywords": [
    "matelint",
    "lint",
    "linter"
  ],
  "bin": {
    "matelint": "bin/matelint.js"
  },
  "files": [
    "bin",
    "lib",
    "package.json",
    "package-lock.json",
    "README.md",
    "rules-description.md"
  ],
  "author": "dev@mate.academy",
  "license": "MIT",
  "homepage": "https://github.com/mate-academy/fed/matelint#README.md",
  "scripts": {
    "start": "rollup -w -c",
    "build": "rm -rf ./bin && rollup -c && node ./shebangify.js && chmod +x ./bin/mateLint.js",
    "lint": "eslint ./src",
    "test": "jest ./src",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "postpublish": "rm -rf ./bin",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.1.0",
    "cosmiconfig": "^7.0.0",
    "parse5": "^7.0.0"
  },
  "devDependencies": {
    "@mate-academy/eslint-config": "0.0.16",
    "@mate-academy/eslint-config-internal": "^1.1.0",
    "@types/commander": "^2.12.2",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^7.21.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.6.3",
    "jest-sinon": "^1.1.0",
    "rollup": "^2.78.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "@rollup/plugin-json": "^4.1.0",
    "table-layout": "^1.0.1",
    "ts-jest": "^26.5.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.7.4"
  }
}
